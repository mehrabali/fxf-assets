<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:odk="http://www.opendatakit.org/xforms"><h:head><h:title>Baseline Household Survey</h:title><model odk:xforms-version="1.0.0"><instance><data id="odk.baseline_household_survey.v1"><start_time/><end_time/><device_id/><subscriber_id/><sim_serial/><phone_number/><interviewer/><hh_info><hh_id/><hh_address/><hh_location/><hh_photo/></hh_info><respondent_info><respondent_name/><dob_known/><respondent_dob/><respondent_age/><respondent_gender/><hh_head/><hh_head_name/><hh_head_age/><hh_head_sex/><contact_number1/><contact_number2/><contact_number3/></respondent_info><hh_member_info><num_hh_members/><num_hh_adults/><num_hh_children/><hh_summary/><hh_member_names_repeat jr:template=""><name_number/><hh_member_name/></hh_member_names_repeat><hh_member_names_repeat><name_number/><hh_member_name/></hh_member_names_repeat><hh_member_info_repeat_count/><hh_member_info_repeat jr:template=""><name_from_earlier/><hh_member_age/><hh_member_sex/></hh_member_info_repeat><hh_member_info_repeat><name_from_earlier/><hh_member_age/><hh_member_sex/></hh_member_info_repeat></hh_member_info><hh_dem_info><hh_ownership/><hh_ownership_other/><hh_religion/><hh_religion_other/><hh_income/><hh_income_other/><hh_language/><hh_language_other/></hh_dem_info><meta><instanceID/></meta></data></instance><instance id="yes_no"><root><item><name>yes</name><label>Yes</label></item><item><name>no</name><label>No</label></item></root></instance><instance id="sex"><root><item><name>male</name><label>Male</label></item><item><name>female</name><label>Female</label></item></root></instance><instance id="ownership"><root><item><name>own</name><label>Owned</label></item><item><name>rent</name><label>Rented/Leased</label></item><item><name>other</name><label>Other</label></item></root></instance><instance id="religion"><root><item><name>muslim</name><label>Muslim</label></item><item><name>christian</name><label>Christian</label></item><item><name>hindu</name><label>Hindu</label></item><item><name>other</name><label>Other</label></item></root></instance><instance id="income"><root><item><name>laborer</name><label>Day / Unskilled laborer</label></item><item><name>farmer</name><label>Farmer / Agriculturalist</label></item><item><name>business</name><label>Business owner</label></item><item><name>govt</name><label>Government employee</label></item><item><name>private</name><label>Private employee</label></item><item><name>other</name><label>Other</label></item></root></instance><instance id="language"><root><item><name>english</name><label>English</label></item><item><name>hindi</name><label>Hindi</label></item><item><name>kannada</name><label>Kannada</label></item><item><name>tamil</name><label>Tamil</label></item><item><name>other</name><label>Other</label></item></root></instance><bind nodeset="/data/start_time" jr:preload="timestamp" type="dateTime" jr:preloadParams="start"/><bind nodeset="/data/end_time" jr:preload="timestamp" type="dateTime" jr:preloadParams="end"/><bind nodeset="/data/device_id" jr:preload="property" type="string" jr:preloadParams="deviceid"/><bind nodeset="/data/subscriber_id" jr:preload="property" type="string" jr:preloadParams="subscriberid"/><bind nodeset="/data/sim_serial" jr:preload="property" type="string" jr:preloadParams="simserial"/><bind nodeset="/data/phone_number" jr:preload="property" type="string" jr:preloadParams="phonenumber"/><bind nodeset="/data/interviewer" type="string" required="true()"/><bind nodeset="/data/hh_info/hh_id" type="int"/><bind nodeset="/data/hh_info/hh_address" type="string"/><bind nodeset="/data/hh_info/hh_location" type="geopoint"/><bind nodeset="/data/hh_info/hh_photo" type="binary"/><bind nodeset="/data/respondent_info/respondent_name" type="string" required="true()"/><bind nodeset="/data/respondent_info/dob_known" type="string" required="true()"/><bind nodeset="/data/respondent_info/respondent_dob" type="date" relevant=" /data/respondent_info/dob_known  = 'yes'" required="true()"/><bind nodeset="/data/respondent_info/respondent_age" type="int" relevant=" /data/respondent_info/dob_known  = 'no'" constraint="(.&gt;=0 and .&lt;=150) or .=999" jr:constraintMsg="Age has to be between 0 and 150, enter 999 if unknown" required="true()"/><bind nodeset="/data/respondent_info/respondent_gender" type="string" required="true()"/><bind nodeset="/data/respondent_info/hh_head" type="string"/><bind nodeset="/data/respondent_info/hh_head_name" type="string" relevant=" /data/respondent_info/hh_head  = 'no'"/><bind nodeset="/data/respondent_info/hh_head_age" type="int" relevant=" /data/respondent_info/hh_head  = 'no'" constraint="(.&gt;=0 and .&lt;=150) or .=999" jr:constraintMsg="Age has to be between 0 and 150, enter 999 if unknown"/><bind nodeset="/data/respondent_info/hh_head_sex" type="string" relevant=" /data/respondent_info/hh_head  = 'no'"/><bind nodeset="/data/respondent_info/contact_number1" type="int"/><bind nodeset="/data/respondent_info/contact_number2" type="int"/><bind nodeset="/data/respondent_info/contact_number3" type="int"/><bind nodeset="/data/hh_member_info/num_hh_members" type="int"/><bind nodeset="/data/hh_member_info/num_hh_adults" type="int" constraint=".&lt;=  /data/hh_member_info/num_hh_members " jr:constraintMsg="Number of adults cannot exceed number of people in the household."/><bind nodeset="/data/hh_member_info/num_hh_children" type="string" calculate=" /data/hh_member_info/num_hh_members  -  /data/hh_member_info/num_hh_adults "/><bind nodeset="/data/hh_member_info/hh_summary" readonly="true()" type="string"/><bind nodeset="/data/hh_member_info/hh_member_names_repeat" relevant=" /data/hh_member_info/num_hh_members !=''"/><bind nodeset="/data/hh_member_info/hh_member_names_repeat/name_number" type="string" calculate="position(..)"/><bind nodeset="/data/hh_member_info/hh_member_names_repeat/hh_member_name" type="string"/><bind nodeset="/data/hh_member_info/hh_member_info_repeat_count" type="string" readonly="true()" calculate="count( /data/hh_member_info/hh_member_names_repeat/hh_member_name )"/><bind nodeset="/data/hh_member_info/hh_member_info_repeat" relevant=" /data/hh_member_info/num_hh_members !=''"/><bind nodeset="/data/hh_member_info/hh_member_info_repeat/name_from_earlier" type="string" calculate="indexed-repeat( /data/hh_member_info/hh_member_names_repeat/hh_member_name ,  /data/hh_member_info/hh_member_names_repeat , position(..))"/><bind nodeset="/data/hh_member_info/hh_member_info_repeat/hh_member_age" type="int"/><bind nodeset="/data/hh_member_info/hh_member_info_repeat/hh_member_sex" type="string"/><bind nodeset="/data/hh_dem_info/hh_ownership" type="string"/><bind nodeset="/data/hh_dem_info/hh_ownership_other" type="string" relevant=" /data/hh_dem_info/hh_ownership  = 'other'"/><bind nodeset="/data/hh_dem_info/hh_religion" type="string"/><bind nodeset="/data/hh_dem_info/hh_religion_other" type="string" relevant=" /data/hh_dem_info/hh_religion  = 'other'"/><bind nodeset="/data/hh_dem_info/hh_income" type="string"/><bind nodeset="/data/hh_dem_info/hh_income_other" type="string" relevant=" /data/hh_dem_info/hh_income  = 'other'"/><bind nodeset="/data/hh_dem_info/hh_language" type="string"/><bind nodeset="/data/hh_dem_info/hh_language_other" type="string" relevant=" /data/hh_dem_info/hh_language  = 'other'"/><bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/></model></h:head><h:body><input ref="/data/interviewer"><label>Interviewer name</label></input><group ref="/data/hh_info"><label>Household information</label><input ref="/data/hh_info/hh_id"><label>Household number</label></input><input ref="/data/hh_info/hh_address"><label>Address or landmark of household</label></input><input ref="/data/hh_info/hh_location"><label>Collect the GPS coordinates of this household</label><hint>Stand directly in front of the household main entrance.</hint></input><upload ref="/data/hh_info/hh_photo" mediatype="image/*"><label>Take a picture of the front of the household</label><hint>Stand far enough back to include the entire structure.</hint></upload></group><group ref="/data/respondent_info"><label>Respondent / Head of Household information</label><input ref="/data/respondent_info/respondent_name"><label>Full name of the respondent</label></input><select1 ref="/data/respondent_info/dob_known"><label>Does respondent know his/her date of birth?</label><itemset nodeset="instance('yes_no')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/respondent_info/respondent_dob"><label>Date of birth of the respondent</label></input><input ref="/data/respondent_info/respondent_age"><label>Age of the respondent</label><hint>If respondent does not know, put 999</hint></input><select1 ref="/data/respondent_info/respondent_gender"><label>Sex of the respondent</label><itemset nodeset="instance('sex')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/respondent_info/hh_head"><label>Is the respondent the head of the household?</label><itemset nodeset="instance('yes_no')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/respondent_info/hh_head_name"><label>Enter the name of the head of household</label></input><input ref="/data/respondent_info/hh_head_age"><label>Age of the head of household</label><hint>If respondent does not know, put 999</hint></input><select1 ref="/data/respondent_info/hh_head_sex"><label>Sex of the head of the household</label><itemset nodeset="instance('sex')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/respondent_info/contact_number1"><label>Contact phone number where head of household can be reached</label></input><input ref="/data/respondent_info/contact_number2"><label>Alternative contact phone number (1)</label></input><input ref="/data/respondent_info/contact_number3"><label>Alternative contact phone number (2)</label></input></group><group ref="/data/hh_member_info"><label>Household member information</label><input ref="/data/hh_member_info/num_hh_members"><label>How many people live in this household?</label><hint>Including the respondent</hint></input><input ref="/data/hh_member_info/num_hh_adults"><label>How many adults live in this household?</label></input><input ref="/data/hh_member_info/hh_summary"><label> The household is composed of <output value=" /data/hh_member_info/num_hh_adults "/> adult(s) and <output value=" /data/hh_member_info/num_hh_children "/> child(ren) </label><hint>If this is incorrect, please adjust the number of people and adults in the household.</hint></input><group ref="/data/hh_member_info/hh_member_names_repeat"><label>Household member names</label><repeat nodeset="/data/hh_member_info/hh_member_names_repeat" jr:count=" /data/hh_member_info/num_hh_members "><input ref="/data/hh_member_info/hh_member_names_repeat/hh_member_name"><label> Name of household member #<output value=" ../name_number "/>? </label></input></repeat></group><group ref="/data/hh_member_info/hh_member_info_repeat"><label>Household member age and sex</label><repeat nodeset="/data/hh_member_info/hh_member_info_repeat" jr:count=" /data/hh_member_info/hh_member_info_repeat_count "><input ref="/data/hh_member_info/hh_member_info_repeat/hh_member_age"><label> Age of <output value=" ../name_from_earlier "/> </label></input><select1 ref="/data/hh_member_info/hh_member_info_repeat/hh_member_sex"><label> Sex of <output value=" ../name_from_earlier "/> </label><itemset nodeset="instance('sex')/root/item"><value ref="name"/><label ref="label"/></itemset></select1></repeat></group></group><group ref="/data/hh_dem_info"><label>Household demographic information</label><select1 ref="/data/hh_dem_info/hh_ownership"><label>This house is â€¦</label><itemset nodeset="instance('ownership')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/hh_dem_info/hh_ownership_other"><label>Please specify other</label></input><select1 ref="/data/hh_dem_info/hh_religion"><label>Primary religion of the household</label><itemset nodeset="instance('religion')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/hh_dem_info/hh_religion_other"><label>Please specify other</label></input><select1 ref="/data/hh_dem_info/hh_income"><label>Occupation of primary income earner in household</label><itemset nodeset="instance('income')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/hh_dem_info/hh_income_other"><label>Please specify other</label></input><select1 ref="/data/hh_dem_info/hh_language"><label>Primary language spoken in the household</label><itemset nodeset="instance('language')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/hh_dem_info/hh_language_other"><label>Please specify other</label></input></group></h:body></h:html>