g"/><bind nodeset="/data/sectionA/interviewer" type="string" required="true()"/><bind nodeset="/data/sectionA/hh_id" type="int" required="true()"/><bind nodeset="/data/sectionA/hh_location" type="geopoint"/><bind nodeset="/data/sectionA/hh_photo" type="binary"/><bind nodeset="/data/sectionA/respondent_name" type="string" required="true()"/><bind nodeset="/data/sectionA/respondent_age" type="int" constraint="(. &gt;= 0 and .&lt;=150) or .=999" jr:constraintMsg="Age has to be between 0 and 150, enter 999 if unknown" required="true()"/><bind nodeset="/data/sectionA/respondent_gender" type="string" required="true()"/><bind nodeset="/data/sectionA/hh_people" type="int" required="true()"/><bind nodeset="/data/sectionA/hh_adult" type="int" constraint=". &lt;=  /data/sectionA/hh_people " jr:constraintMsg="Has to be less than or equal to the number of people in the household"/><bind nodeset="/data/sectionA/hh_children" type="string" calculate=" /data/sectionA/hh_people  -  /data/sectionA/hh_adult "/><bind nodeset="/data/sectionA/hh_summary" type="string"/><bind nodeset="/data/sectionA/hh_ownership" type="string"/><bind nodeset="/data/sectionA/hh_ownership_other" type="string" relevant=" /data/sectionA/hh_ownership  = 'other'"/><bind nodeset="/data/sectionB/hh_water_source" type="string" required="true()"/><bind nodeset="/data/sectionB/hh_water_source_other" type="string" relevant="selected( /data/sectionB/hh_water_source , 'other')"/><bind nodeset="/data/sectionB/hh_drinking" type="string"/><bind nodeset="/data/sectionB/hh_cooking" type="string"/><bind nodeset="/data/sectionB/hh_bathing" type="string"/><bind nodeset="/data/sectionB/hh_drinking_quality" type="string"/><bind nodeset="/data/sectionB/hh_cooking_quality" type="string"/><bind nodeset="/data/sectionB/hh_bathing_quality" type="string"/><bind nodeset="/data/sectionB/hh_drinking_reliable" type="string"/><bind nodeset="/data/sectionB/hh_cooking_reliable" type="string"/><bind nodeset="/data/sectionB/hh_bathing_reliable" type="string"/><bind nodeset="/data/sectionB/seven_days_access" type="string"/><bind nodeset="/data/sectionB/worry_water" type="string"/><bind nodeset="/data/sectionB/thirty_days_dispute" type="string"/><bind nodeset="/data/sectionB/safer_water" type="string"/><bind nodeset="/data/sectionB/safer_water_method" type="string" relevant=" /data/sectionB/safer_water  = 'yes'"/><bind nodeset="/data/sectionB/safer_water_method_other" type="string" relevant="selected( /data/sectionB/safer_water_method , 'other')"/><bind nodeset="/data/sectionB/hh_bottle_expenditure" type="decimal" relevant="selected( /data/sectionB/hh_water_source , 'bottled')"/><bind nodeset="/data/sectionB/hh_bottle_currency" type="string" relevant="selected( /data/sectionB/hh_water_source , 'bottled')"/><bind nodeset="/data/sectionB/percent_weekly_income" type="int" constraint=".&gt;=0 and .&lt;=100"/><bind nodeset="/data/sectionC/county" type="string"/><bind nodeset="/data/sectionC/district" type="string"/><bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/></model></h:head><h:body><input ref="/data/note_consent"><label>Administer informed consent. If subject agrees to participate, proceed with survey.</label></input><group ref="/data/sectionA"><label>Section A - Household Information</label><input ref="/data/sectionA/interviewer"><label>A1 Interviewer name</label></input><input ref="/data/sectionA/hh_id"><label>A2 Enter the household number</label></input><input ref="/data/sectionA/hh_location"><label>A3 Collect the GPS coordinates of this household</label></input><upload ref="/data/sectionA/hh_photo" mediatype="image/*"><label>A4 Take a picture of the front of the household</label></upload><input ref="/data/sectionA/respondent_name"><label>A5 Enter the full name of the respondent</label></input><input ref="/data/sectionA/respondent_age"><label>A6 Enter the age of the respondent</label><hint>If respondent does not know, put 999</hint></input><select1 ref="/data/sectionA/respondent_gender"><label>A7 Enter the gender of the respondent</label><itemset nodeset="instance('gender_list')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/sectionA/hh_people"><label>A8 How many people live in this household?</label><hint>Including the respondent</hint></input><input ref="/data/sectionA/hh_adult"><label>A9 How many adults live in this household?</label></input><trigger ref="/data/sectionA/hh_summary"><label> This household is composed of <output value=" /data/sectionA/hh_adult "/> adults and <output value=" /data/sectionA/hh_children "/> child(ren) </label></trigger><select1 ref="/data/sectionA/hh_ownership"><label>A10 This house is â€¦</label><itemset nodeset="instance('ownership_list')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/sectionA/hh_ownership_other"><label>Please specify other</label></input></group><group ref="/data/sectionB"><label>Section B - Water use practices</label><select ref="/data/sectionB/hh_water_source"><label>B1 What are all of the sources of water that you use at home?</label><hint>(select all that apply)</hint><itemset nodeset="instance('water_sources')/root/item"><value ref="name"/><label ref="label"/></itemset></select><input ref="/data/sectionB/hh_water_source_other"><label>Please specify other</label></input><select1 ref="/data/sectionB/hh_drinking"><label>B2 Of these, what is your primary source of water for drinking?</label><itemset nodeset="instance('water_sources2')/root/item"><value ref="name"/><label ref="jr:itext(itextId)"/></itemset></select1><select1 ref="/data/sectionB/hh_cooking"><label>B3 Of these, what is your primary source of water for cooking?</label><itemset nodeset="instance('water_sources2')/root/item"><value ref="name"/><label ref="jr:itext(itextId)"/></itemset></select1><select1 ref="/data/sectionB/hh_bathing"><label>B4 Of these, what is your primary source of water for bathing?</label><itemset nodeset="instance('water_sources2')/root/item"><value ref="name"/><label ref="jr:itext(itextId)"/></itemset></select1><select1 ref="/data/sectionB/hh_drinking_quality" appearance="likert"><label>B5 How would you rank the quality of the water you use for drinking?</label><itemset nodeset="instance('quality')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/sectionB/hh_cooking_quality" appearance="likert"><label>B6 How would you rank the quality of the water you use for cooking?</label><itemset nodeset="instance('quality')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/sectionB/hh_bathing_quality" appearance="likert"><label>B7 How would you rank the quality of the water you use for bathing?</label><itemset nodeset="instance('quality')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/sectionB/hh_drinking_reliable" appearance="likert"><label>B8 How reliable is the supply of water that you use for drinking?</label><itemset nodeset="instance('reliable')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/sectionB/hh_cooking_reliable" appearance="likert"><label>B9 How reliable is the supply of water that you use for cooking?</label><itemset nodeset="instance('reliable')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/sectionB/hh_bathing_reliable" appearance="likert"><label>B10 How reliable is the supply of water that you use for bathing?</label><itemset nodeset="instance('reliable')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/sectionB/seven_days_access"><label>B11 In the past 7 days, have you or anyone in your household not had enough water to meet their daily needs?</label><itemset nodeset="instance('y_n')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/sectionB/worry_water" appearance="likert"><label>B12 How often would you say you worry about not having enough water to meet all of the household's daily needs?</label><itemset nodeset="instance('worry')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/sectionB/thirty_days_dispute"><label>B13 In the past 30 days, have you or anyone in your household gotten into a dispute over water access or usage?</label><itemset nodeset="instance('y_n')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/sectionB/safer_water"><label>B14 Do you do anything to your drinking water to make it safer to drink?</label><itemset nodeset="instance('y_n')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select ref="/data/sectionB/safer_water_method"><label>B15 What do you do to your water to make it safer to drink?</label><hint>(select all that apply)</hint><itemset nodeset="instance('safe_water')/root/item"><value ref="name"/><label ref="label"/></itemset></select><input ref="/data/sectionB/safer_water_method_other"><label>Please specify other</label></input><input ref="/data/sectionB/hh_bottle_expenditure"><label>How much do you spend on bottled water per month?</label></input><select1 ref="/data/sectionB/hh_bottle_currency"><label>Currency (specify one)</label><itemset nodeset="instance('currencies')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/sectionB/percent_weekly_income"><label>B16 What percentage of the household's weekly income is spent on obtaining water (for all uses)?</label></input></group><group ref="/data/sectionC"><label>Section C - Geographic Location</label><select1 ref="/data/sectionC/county"><label>C1 County</label><itemset nodeset="instance('counties')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/sectionC/district"><label>C2 District</label><itemset nodeset="instance('districts')/root/item[county= /data/sectionC/county ]"><value ref="name"/><label ref="label"/></itemset></select1></group></h:body></h:html>