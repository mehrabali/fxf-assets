ight" type="int"/><bind nodeset="/data/b_weight_calc" type="string" calculate="if(selected( /data/language , '1'), concat(&quot;Birthweight:&quot;, &quot; &quot;,  /data/b_weight , &quot;g;&quot;, &quot; &quot;), if(selected( /data/language , '2'), concat(&quot;হ্যাঁt:&quot;, &quot; &quot;,  /data/b_weight , &quot;g;&quot;, &quot; &quot;), 'unspecified'))"/><bind nodeset="/data/note3" readonly="true()" type="string"/><bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/></model></h:head><h:body><select1 ref="/data/language"><label ref="jr:itext('/data/language:label')"/><itemset nodeset="instance('language')/root/item"><value ref="name"/><label ref="jr:itext(itextId)"/></itemset></select1><select1 ref="/data/mouth"><label ref="jr:itext('/data/mouth:label')"/><itemset nodeset="instance('mouth')/root/item"><value ref="name"/><label ref="jr:itext(itextId)"/></itemset></select1><select1 ref="/data/areola"><label ref="jr:itext('/data/areola:label')"/><itemset nodeset="instance('areola')/root/item"><value ref="name"/><label ref="jr:itext(itextId)"/></itemset></select1><input ref="/data/note1"><label ref="jr:itext('/data/note1:label')"/></input><input ref="/data/note2"><label ref="jr:itext('/data/note2:label')"/></input><input ref="/data/b_weight"><label ref="jr:itext('/data/b_weight:label')"/></input><input ref="/data/note3"><label ref="jr:itext('/data/note3:label')"/></input></h:body></h:html>