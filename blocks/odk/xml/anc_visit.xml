<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:odk="http://www.opendatakit.org/xforms"><h:head><h:title>Antenatal Care (ANC) Visit</h:title><model odk:xforms-version="1.0.0"><instance><data id="ANC_Visitodk.anc_visit.v1"><today/><start_time/><end_time/><device_id/><subscriber_id/><sim_serial/><phone_number/><woman_name/><woman_id/><anc_number/><anc_visit_date/><anc_visit_place/><anc_visit_person/><risk_observed/><risk_observed_other/><vitals_taken/><bp_systolic/><bp_diastolic/><pulse_rate/><temperature/><weight/><abdominal_exam/><abdominals_abnormal/><multiple_pregnancy/><malpresentation_of_fetus/><jaundice/><pregnancy_induced_hypertension/><multiple_preg/><malpresentation/><is_high_risk/><high_risk_reason/><high_risk_note/><meta><instanceID/></meta></data></instance><instance id="anc_number"><root><item><name>1</name><label>ANC 1</label></item><item><name>2</name><label>ANC 2</label></item><item><name>3</name><label>ANC 3</label></item><item><name>4</name><label>ANC 4</label></item><item><name>5</name><label>Other ANC visit</label></item></root></instance><instance id="anc_place"><root><item><name>sub_center</name><label>Sub Center</label></item><item><name>phc</name><label>Primary Health Center</label></item><item><name>chc</name><label>Community Health Center</label></item><item><name>hospital</name><label>District Hospital</label></item><item><name>other</name><label>Other</label></item></root></instance><instance id="anc_person"><root><item><name>nurse</name><label>Nurse</label></item><item><name>midwife</name><label>Midwife</label></item><item><name>doctor</name><label>Medical Doctor</label></item><item><name>other</name><label>Other</label></item></root></instance><instance id="risk_symptoms"><root><item><name>pallor</name><label>Pallor</label></item><item><name>swelling</name><label>Swelling / Edema</label></item><item><name>bleeding</name><label>Bleeding</label></item><item><name>jaundice</name><label>Jaundice</label></item><item><name>convulsions</name><label>Fits / Convulsions</label></item><item><name>other</name><label>Other</label></item><item><name>none</name><label>No risk symptoms observed</label></item></root></instance><instance id="yes_no"><root><item><name>yes</name><label>Yes</label></item><item><name>no</name><label>No</label></item></root></instance><instance id="vital_measurements"><root><item><name>bp</name><label>BP</label></item><item><name>pulse_rate</name><label>Pulse rate</label></item><item><name>temperature</name><label>Temperature</label></item><item><name>weight</name><label>Weight</label></item></root></instance><instance id="abdominal_measurements"><root><item><name>abdominal_height</name><label>Abdominal height</label></item><item><name>fetal_heart_rate</name><label>Fetal heart rate</label></item><item><name>fetal_movement</name><label>Fetal movement</label></item></root></instance><bind nodeset="/data/today" jr:preload="date" type="date" jr:preloadParams="today"/><bind nodeset="/data/start_time" jr:preload="timestamp" type="dateTime" jr:preloadParams="start"/><bind nodeset="/data/end_time" jr:preload="timestamp" type="dateTime" jr:preloadParams="end"/><bind nodeset="/data/device_id" jr:preload="property" type="string" jr:preloadParams="deviceid"/><bind nodeset="/data/subscriber_id" jr:preload="property" type="string" jr:preloadParams="subscriberid"/><bind nodeset="/data/sim_serial" jr:preload="property" type="string" jr:preloadParams="simserial"/><bind nodeset="/data/phone_number" jr:preload="property" type="string" jr:preloadParams="phonenumber"/><bind nodeset="/data/woman_name" type="string" required="true()"/><bind nodeset="/data/woman_id" type="int" required="true()"/><bind nodeset="/data/anc_number" type="string" required="true()"/><bind nodeset="/data/anc_visit_date" type="date" constraint=".&lt;= /data/today " required="true()"/><bind nodeset="/data/anc_visit_place" type="string" required="true()"/><bind nodeset="/data/anc_visit_person" type="string" required="true()"/><bind nodeset="/data/risk_observed" type="string" constraint="(selected(., 'none') and (count-selected(.) = 1) or (not(selected(., 'none'))))"/><bind nodeset="/data/risk_observed_other" type="string" relevant="selected( /data/risk_observed , 'other')"/><bind nodeset="/data/vitals_taken" type="string"/><bind nodeset="/data/bp_systolic" type="int" relevant="selected( /data/vitals_taken , 'bp')" constraint=".&gt;0"/><bind nodeset="/data/bp_diastolic" type="int" relevant="selected( /data/vitals_taken , 'bp')" constraint=".&gt;0"/><bind nodeset="/data/pulse_rate" type="int" relevant="selected( /data/vitals_taken , 'pulse_rate')" constraint=".&gt;0"/><bind nodeset="/data/temperature" type="decimal" relevant="selected( /data/vitals_taken , 'temperature')" constraint=".&gt;0"/><bind nodeset="/data/weight" type="decimal" relevant="selected( /data/vitals_taken , 'weight')" constraint=".&gt;0"/><bind nodeset="/data/abdominal_exam" type="string"/><bind nodeset="/data/abdominals_abnormal" type="string" relevant=" /data/abdominal_exam  = 'yes'"/><bind nodeset="/data/multiple_pregnancy" type="string" relevant=" /data/abdominal_exam  = 'yes'"/><bind nodeset="/data/malpresentation_of_fetus" type="string" relevant=" /data/abdominal_exam  = 'yes'"/><bind nodeset="/data/jaundice" type="string" calculate="if(selected( /data/risk_observed , 'jaundice'), 'Jaundice', '')"/><bind nodeset="/data/pregnancy_induced_hypertension" type="string" calculate="if( /data/bp_systolic  &gt;= 140 or  /data/bp_diastolic  &gt;= 90, 'PIH', '')"/><bind nodeset="/data/multiple_preg" type="string" calculate="if( /data/multiple_pregnancy  = 'yes', 'Multiple_Pregnancy', '')"/><bind nodeset="/data/malpresentation" type="string" calculate="if( /data/malpresentation_of_fetus  = 'yes', 'Malpresentation', '')"/><bind nodeset="/data/is_high_risk" type="string" calculate="if( /data/pregnancy_induced_hypertension  != '' or  /data/jaundice  != '' or  /data/multiple_preg  != '' or  /data/malpresentation  != '', 'yes', 'no')"/><bind nodeset="/data/high_risk_reason" type="string" calculate="concat( /data/pregnancy_induced_hypertension , ' ',  /data/jaundice , ' ',  /data/multiple_preg , ' ',  /data/malpresentation )"/><bind nodeset="/data/high_risk_note" readonly="true()" type="string" relevant=" /data/is_high_risk  = 'yes'"/><bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/></model></h:head><h:body><input ref="/data/woman_name"><label>Woman's name?</label></input><input ref="/data/woman_id"><label>Woman's ID number:</label></input><select1 ref="/data/anc_number"><label>Which ANC visit is this?</label><itemset nodeset="instance('anc_number')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/anc_visit_date"><label>ANC visit date?</label></input><select1 ref="/data/anc_visit_place"><label>Place of ANC visit?</label><itemset nodeset="instance('anc_place')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/anc_visit_person"><label>Who did ANC visit?</label><itemset nodeset="instance('anc_person')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select ref="/data/risk_observed"><label>Does the woman have any risk symptoms?</label><itemset nodeset="instance('risk_symptoms')/root/item"><value ref="name"/><label ref="label"/></itemset></select><input ref="/data/risk_observed_other"><label>What other risk symptom do you observe?</label></input><select ref="/data/vitals_taken"><label>Which vital measurements were taken?</label><itemset nodeset="instance('vital_measurements')/root/item"><value ref="name"/><label ref="label"/></itemset></select><input ref="/data/bp_systolic"><label>BP (systolic)</label></input><input ref="/data/bp_diastolic"><label>BP (diastolic)</label></input><input ref="/data/pulse_rate"><label>Pulse rate</label></input><input ref="/data/temperature"><label>Temperature (Â°C)</label></input><input ref="/data/weight"><label>Weight (in kgs)</label></input><select1 ref="/data/abdominal_exam"><label>Did you conduct an abdominal exam?</label><itemset nodeset="instance('yes_no')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select ref="/data/abdominals_abnormal"><label>Any abdominal measurements abnormal?</label><itemset nodeset="instance('abdominal_measurements')/root/item"><value ref="name"/><label ref="label"/></itemset></select><select1 ref="/data/multiple_pregnancy"><label>Is this a multiple pregnancy (twins, triplets)?</label><itemset nodeset="instance('yes_no')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/malpresentation_of_fetus"><label>Is there malpresentation of the fetus?</label><itemset nodeset="instance('yes_no')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/high_risk_note"><label><output value=" /data/woman_name "/> has a high risk pregnancy due to: <output value=" /data/high_risk_reason "/> </label></input></h:body></h:html>