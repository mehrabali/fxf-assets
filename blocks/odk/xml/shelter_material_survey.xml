<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:odk="http://www.opendatakit.org/xforms"><h:head><h:title>Survey on Household Characteristics</h:title><model odk:xforms-version="1.0.0"><instance><data id="odk.shelter_material_survey.v1"><start_time/><end_time/><section1><hhid/><date/><state/><geo/><other_geo/><address/><hhgps/><hhpicture/></section1><section2><note1/><note2/><name/><hhposition/><hhsize/><hhadults/><hhboys/><hhgirls/></section2><section3><note3/><contact/><phone/></section3><section4><hh_roof/><other_hh_roof/><hh_wall/><other_hh_wall/><hh_floor/><other_hh_floor/><hh_toilet/><hh_light/><other_hh_light/><hh_other/><hh_comparison/><notes/></section4><meta><instanceID/></meta></data></instance><instance id="geo_list"><root><item><name>urban</name><label>Urban</label></item><item><name>peri</name><label>Peri-urban</label></item><item><name>rural</name><label>Rural</label></item><item><name>other_geolist</name><label>Other</label></item></root></instance><instance id="state"><root><item><name>kad</name><label>Kaduna</label></item><item><name>kan</name><label>Kano</label></item><item><name>kat</name><label>Katsina</label></item><item><name>jig</name><label>Jigawa</label></item><item><name>yob</name><label>Yobe</label></item><item><name>zam</name><label>Zamfara</label></item></root></instance><instance id="hhpos"><root><item><name>f_head</name><label>Female head of household</label></item><item><name>f_oldest</name><label>Oldest female in household</label></item><item><name>f_other</name><label>Other female</label></item><item><name>m_head</name><label>Male head of household</label></item><item><name>m_oldest</name><label>Oldest male in household</label></item><item><name>m_other</name><label>Other male</label></item></root></instance><instance id="yesno"><root><item><name>yes</name><label>Yes</label></item><item><name>no</name><label>No</label></item></root></instance><instance id="roof"><root><item><name>metal</name><label>Metal sheets</label></item><item><name>brick</name><label>Brick</label></item><item><name>thatch</name><label>Thatch / Palm leaf</label></item><item><name>tiles</name><label>Roofing tiles</label></item><item><name>other_roof</name><label>Other</label></item></root></instance><instance id="wall"><root><item><name>tin</name><label>Tin</label></item><item><name>brick</name><label>Brick</label></item><item><name>mud</name><label>Mud</label></item><item><name>concrete</name><label>Concrete</label></item><item><name>other_wall</name><label>Other</label></item></root></instance><instance id="floor"><root><item><name>earth</name><label>Earth / Sand</label></item><item><name>tiles</name><label>Ceramic tiles</label></item><item><name>cement</name><label>Cement</label></item><item><name>wood</name><label>Wood</label></item><item><name>other_floor</name><label>Other</label></item></root></instance><instance id="light"><root><item><name>electricity</name><label>Electricity</label></item><item><name>solar</name><label>Solar</label></item><item><name>gas</name><label>LPG gas</label></item><item><name>oil</name><label>Oil lamp</label></item><item><name>dark</name><label>Dark (only natural light)</label></item><item><name>other_light</name><label>Other</label></item></root></instance><instance id="pov"><root><item><name>poorer</name><label>Poorer than others</label></item><item><name>same</name><label>About the same as others</label></item><item><name>wealthier</name><label>Wealthier than others</label></item></root></instance><bind nodeset="/data/start_time" jr:preload="timestamp" type="dateTime" jr:preloadParams="start"/><bind nodeset="/data/end_time" jr:preload="timestamp" type="dateTime" jr:preloadParams="end"/><bind nodeset="/data/section1/hhid" type="int"/><bind nodeset="/data/section1/date" type="date"/><bind nodeset="/data/section1/state" type="string"/><bind nodeset="/data/section1/geo" type="string"/><bind nodeset="/data/section1/other_geo" type="string" relevant=" /data/section1/geo ='other_geolist'"/><bind nodeset="/data/section1/address" type="string"/><bind nodeset="/data/section1/hhgps" type="geopoint"/><bind nodeset="/data/section1/hhpicture" type="binary"/><bind nodeset="/data/section2/note1" readonly="true()" type="string"/><bind nodeset="/data/section2/note2" readonly="true()" type="string"/><bind nodeset="/data/section2/name" type="string"/><bind nodeset="/data/section2/hhposition" type="string"/><bind nodeset="/data/section2/hhsize" type="int"/><bind nodeset="/data/section2/hhadults" type="int"/><bind nodeset="/data/section2/hhboys" type="int"/><bind nodeset="/data/section2/hhgirls" type="int"/><bind nodeset="/data/section3/note3" readonly="true()" type="string"/><bind nodeset="/data/section3/contact" type="string"/><bind nodeset="/data/section3/phone" type="string" relevant=" /data/section3/contact  = 'yes'"/><bind nodeset="/data/section4/hh_roof" type="string"/><bind nodeset="/data/section4/other_hh_roof" type="string" relevant=" /data/section4/hh_roof ='other_roof'"/><bind nodeset="/data/section4/hh_wall" type="string"/><bind nodeset="/data/section4/other_hh_wall" type="string" relevant=" /data/section4/hh_wall ='other_wall'"/><bind nodeset="/data/section4/hh_floor" type="string"/><bind nodeset="/data/section4/other_hh_floor" type="string" relevant=" /data/section4/hh_floor ='other_floor'"/><bind nodeset="/data/section4/hh_toilet" type="string"/><bind nodeset="/data/section4/hh_light" type="string"/><bind nodeset="/data/section4/other_hh_light" type="string" relevant=" /data/section4/hh_floor ='other_hh_light'"/><bind nodeset="/data/section4/hh_other" type="string"/><bind nodeset="/data/section4/hh_comparison" type="string"/><bind nodeset="/data/section4/notes" type="string"/><bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/></model></h:head><h:body><group ref="/data/section1"><label>Section 1. Record this information before approaching the household</label><input ref="/data/section1/hhid"><label>Please assign a unique household ID.</label></input><input ref="/data/section1/date"><label>What is today's date?</label></input><select1 ref="/data/section1/state"><label>What state are you in?</label><itemset nodeset="instance('state')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/section1/geo"><label>Is this area rural, urban, peri-urban, or other?</label><itemset nodeset="instance('geo_list')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/section1/other_geo"><label>Specify other</label></input><input ref="/data/section1/address"><label>Please enter the address or description of the home location.</label></input><input ref="/data/section1/hhgps"><label>Please record the GPS coordinates of this home.</label></input><upload ref="/data/section1/hhpicture" mediatype="image/*"><label>If available, you may want to take a photo of the front of the house.</label></upload></group><group ref="/data/section2"><label>Section 2. Household information</label><input ref="/data/section2/note1"><label>Begin the interview with a short introduction about who you are, the program, and the purpose of the survey.</label></input><input ref="/data/section2/note2"><label>Ask to interview the female in charge of the household or the oldest available female. If not available, male is ok.</label></input><input ref="/data/section2/name"><label>What is your name?</label></input><select1 ref="/data/section2/hhposition"><label>What is your position in the household?</label><itemset nodeset="instance('hhpos')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/section2/hhsize"><label>How many people live in this household most of the time?</label></input><input ref="/data/section2/hhadults"><label>How many of the people who live here are adults (age 18 or over)?</label></input><input ref="/data/section2/hhboys"><label>How many boys under age 18 live here?</label><hint>Don't forget babies, visitors, and household help.</hint></input><input ref="/data/section2/hhgirls"><label>How many girls under age 18 live here?</label><hint>Don't forget babies, visitors, and household help.</hint></input></group><group ref="/data/section3"><label>Section 3. Contact information</label><input ref="/data/section3/note3"><label>Thank you for participating in this survey. Here is my contact information if you want to reach me.</label></input><select1 ref="/data/section3/contact"><label>If you're comfortable, is there any way we can get in touch with you?</label><itemset nodeset="instance('yesno')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/section3/phone"><label>Enter phone number or other contact.</label></input></group><group ref="/data/section4"><label>Section 4. Interviewer observations</label><select1 ref="/data/section4/hh_roof"><label>The roof of this house is made of:</label><itemset nodeset="instance('roof')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/section4/other_hh_roof"><label>Specify other</label></input><select1 ref="/data/section4/hh_wall"><label>The walls of the house are made of:</label><itemset nodeset="instance('wall')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/section4/other_hh_wall"><label>Specify other</label></input><select1 ref="/data/section4/hh_floor"><label>The floor of the house is made of:</label><itemset nodeset="instance('floor')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/section4/other_hh_floor"><label>Specify other</label></input><select1 ref="/data/section4/hh_toilet"><label>Does the house appear to have access to improved toilet facilities?</label><itemset nodeset="instance('yesno')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><select1 ref="/data/section4/hh_light"><label>What is the main source of light in the house?</label><itemset nodeset="instance('light')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/section4/other_hh_light"><label>Specify other</label></input><input ref="/data/section4/hh_other"><label>Other identifying characteristics of house:</label></input><select1 ref="/data/section4/hh_comparison" appearance="likert"><label>Compared to other houses in the community, how would you say this one compares?</label><itemset nodeset="instance('pov')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/section4/notes"><label>Did you see anything that should be followed up or any other notes you wish to make?</label></input></group></h:body></h:html>