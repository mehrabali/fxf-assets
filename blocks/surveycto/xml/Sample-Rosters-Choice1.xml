<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Sample Form - Rosters - Choice 1</h:title>
    <model>
      <itext>
        <translation default="true()" lang="english">
          <text id="static_instance-yesno-0">
            <value>Yes</value>
          </text>
          <text id="static_instance-yesno-1">
            <value>No</value>
          </text>
          <text id="static_instance-gender-0">
            <value>Male</value>
          </text>
          <text id="static_instance-gender-1">
            <value>Female</value>
          </text>
          <text id="static_instance-hhmember-0">
            <value><output value=" /scto.samplerostersc1.v1/fam_name1 "/></value>
          </text>
          <text id="static_instance-hhmember-1">
            <value><output value=" /scto.samplerostersc1.v1/fam_name2 "/></value>
          </text>
          <text id="static_instance-hhmember-2">
            <value><output value=" /scto.samplerostersc1.v1/fam_name3 "/></value>
          </text>
          <text id="static_instance-hhmember-3">
            <value><output value=" /scto.samplerostersc1.v1/fam_name4 "/></value>
          </text>
          <text id="static_instance-hhmember-4">
            <value><output value=" /scto.samplerostersc1.v1/fam_name5 "/></value>
          </text>
        </translation>
      </itext>
      <instance>
        <scto.samplerostersc1.v1 id="scto.samplerostersc1.v1" version="2509041634">
          <starttime/>
          <endtime/>
          <deviceid/>
          <devicephonenum/>
          <device_info/>
          <duration/>
          <intronote/>
          <consent/>
          <numfamily/>
          <fam_name1/>
          <fam_name2/>
          <fam_name3/>
          <fam_name4/>
          <fam_name5/>
          <numworkers/>
          <repeatintro/>
          <worker_group_count/>
          <worker_group jr:template="">
            <worker_hhmember/>
            <worker_place/>
          </worker_group>
          <conclusion/>
          <meta>
            <instanceID/>
          </meta>
        </scto.samplerostersc1.v1>
      </instance>
      <instance id="yesno">
        <root>
          <item>
            <itextId>static_instance-yesno-0</itextId>
            <name>1</name>
          </item>
          <item>
            <itextId>static_instance-yesno-1</itextId>
            <name>0</name>
          </item>
        </root>
      </instance>
      <instance id="gender">
        <root>
          <item>
            <itextId>static_instance-gender-0</itextId>
            <name>0</name>
          </item>
          <item>
            <itextId>static_instance-gender-1</itextId>
            <name>1</name>
          </item>
        </root>
      </instance>
      <instance id="hhmember">
        <root>
          <item>
            <itextId>static_instance-hhmember-0</itextId>
            <name>1</name>
            <filter>1</filter>
          </item>
          <item>
            <itextId>static_instance-hhmember-1</itextId>
            <name>2</name>
            <filter>2</filter>
          </item>
          <item>
            <itextId>static_instance-hhmember-2</itextId>
            <name>3</name>
            <filter>3</filter>
          </item>
          <item>
            <itextId>static_instance-hhmember-3</itextId>
            <name>4</name>
            <filter>4</filter>
          </item>
          <item>
            <itextId>static_instance-hhmember-4</itextId>
            <name>5</name>
            <filter>5</filter>
          </item>
        </root>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/scto.samplerostersc1.v1/starttime" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/scto.samplerostersc1.v1/endtime" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/scto.samplerostersc1.v1/deviceid" type="string"/>
      <bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/scto.samplerostersc1.v1/devicephonenum" type="string"/>
      <bind calculate="device-info()" nodeset="/scto.samplerostersc1.v1/device_info" type="string"/>
      <bind calculate="duration()" nodeset="/scto.samplerostersc1.v1/duration" type="string"/>
      <bind nodeset="/scto.samplerostersc1.v1/intronote" original="note" readonly="true()" type="string"/>
      <bind nodeset="/scto.samplerostersc1.v1/consent" required="true()" type="select1"/>
      <bind constraint=".&gt;=0 and .&lt;=5" jr:constraintMsg="Please enter a number between 0 and 5." nodeset="/scto.samplerostersc1.v1/numfamily" relevant=" /scto.samplerostersc1.v1/consent =1" required="true()" type="int"/>
      <bind nodeset="/scto.samplerostersc1.v1/fam_name1" relevant=" /scto.samplerostersc1.v1/consent =1 and  /scto.samplerostersc1.v1/numfamily &gt;=1" required="true()" type="string"/>
      <bind nodeset="/scto.samplerostersc1.v1/fam_name2" relevant=" /scto.samplerostersc1.v1/consent =1 and  /scto.samplerostersc1.v1/numfamily &gt;=2" required="true()" type="string"/>
      <bind nodeset="/scto.samplerostersc1.v1/fam_name3" relevant=" /scto.samplerostersc1.v1/consent =1 and  /scto.samplerostersc1.v1/numfamily &gt;=3" required="true()" type="string"/>
      <bind nodeset="/scto.samplerostersc1.v1/fam_name4" relevant=" /scto.samplerostersc1.v1/consent =1 and  /scto.samplerostersc1.v1/numfamily &gt;=4" required="true()" type="string"/>
      <bind nodeset="/scto.samplerostersc1.v1/fam_name5" relevant=" /scto.samplerostersc1.v1/consent =1 and  /scto.samplerostersc1.v1/numfamily &gt;=5" required="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;= /scto.samplerostersc1.v1/numfamily " jr:constraintMsg="Please enter between 0 and the total number of household members." nodeset="/scto.samplerostersc1.v1/numworkers" relevant=" /scto.samplerostersc1.v1/consent =1" required="true()" type="int"/>
      <bind nodeset="/scto.samplerostersc1.v1/repeatintro" original="note" readonly="true()" relevant=" /scto.samplerostersc1.v1/consent =1 and  /scto.samplerostersc1.v1/numworkers &gt;0" type="string"/>
      <bind calculate=" /scto.samplerostersc1.v1/numworkers " nodeset="/scto.samplerostersc1.v1/worker_group_count" readonly="true()" type="string"/>
      <bind nodeset="/scto.samplerostersc1.v1/worker_group" relevant=" /scto.samplerostersc1.v1/consent =1"/>
      <bind nodeset="/scto.samplerostersc1.v1/worker_group/worker_hhmember" required="true()" type="select1"/>
      <bind nodeset="/scto.samplerostersc1.v1/worker_group/worker_place" required="true()" type="string"/>
      <bind nodeset="/scto.samplerostersc1.v1/conclusion" original="note" readonly="true()" relevant=" /scto.samplerostersc1.v1/consent =1" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/scto.samplerostersc1.v1/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input appearance="intro" ref="/scto.samplerostersc1.v1/intronote">
      <label>Welcome to the sample rosters form (choice variation 1). Please swipe forward to continue.</label>
    </input>
    <select1 ref="/scto.samplerostersc1.v1/consent">
      <label>Would you like to take the survey?</label>
      <item>
        <label>Yes</label>
        <value>1</value>
      </item>
      <item>
        <label>No</label>
        <value>0</value>
      </item>
    </select1>
    <input ref="/scto.samplerostersc1.v1/numfamily">
      <label>How many family members live with you?</label>
    </input>
    <input ref="/scto.samplerostersc1.v1/fam_name1">
      <label>What is the name of the first other family member who lives with you?</label>
    </input>
    <input ref="/scto.samplerostersc1.v1/fam_name2">
      <label>What is the name of the second other family member who lives with you?</label>
    </input>
    <input ref="/scto.samplerostersc1.v1/fam_name3">
      <label>What is the name of the third other family member who lives with you?</label>
    </input>
    <input ref="/scto.samplerostersc1.v1/fam_name4">
      <label>What is the name of the fourth other family member who lives with you?</label>
    </input>
    <input ref="/scto.samplerostersc1.v1/fam_name5">
      <label>What is the name of the fifth other family member who lives with you?</label>
    </input>
    <input ref="/scto.samplerostersc1.v1/numworkers">
      <label>How many of your family members work outside the home?</label>
    </input>
    <input ref="/scto.samplerostersc1.v1/repeatintro">
      <label>Please answer the following questions for each family member who works outside the home.</label>
    </input>
    <group ref="/scto.samplerostersc1.v1/worker_group">
      <label>Worker</label>
      <repeat jr:count=" /scto.samplerostersc1.v1/worker_group_count " nodeset="/scto.samplerostersc1.v1/worker_group">
        <select1 ref="/scto.samplerostersc1.v1/worker_group/worker_hhmember">
          <label>Which household member?</label>
          <itemset nodeset="instance('hhmember')/root/item[ /scto.samplerostersc1.v1/numfamily &gt;=filter]">
            <value ref="name"/>
            <label ref="jr:itext(itextId)"/>
          </itemset>
        </select1>
        <input ref="/scto.samplerostersc1.v1/worker_group/worker_place">
          <label>Where does he or she work?</label>
        </input>
      </repeat>
    </group>
    <input ref="/scto.samplerostersc1.v1/conclusion">
      <label>Thank you. That concludes the survey.</label>
    </input>
  </h:body>
</h:html>
